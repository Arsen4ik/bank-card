generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("POSTGRES_PRISMA_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

model card {
    id     String @id @default(uuid())
    name   String
    number String @unique
    month  String
    year   String
    cvc    String
}
